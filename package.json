{
  "name": "kickstart-react-typescript",
  "version": "1.0.0",
  "description": "Kickstarter for a React project using Typescript and Parcel as the Bundler",
  "main": "index.js",
  "repository": "https://github.com/lmarzy/kickstart-react-typescript.git",
  "author": "lmarzy <me@phz3.net>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist .cache",
    "test": "jest",
    "cypress:open": "cypress open",
    "dev": "yarn clean && parcel ./src/index.html",
    "build": "yarn clean && parcel build ./src/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "assetsPath": "src/assets",
  "dependencies": {
    "apollo-boost": "^0.1.28",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "parcel-plugin-asset-copier": "^1.0.0",
    "react": "^16.8.2",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.5",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.8",
    "cypress": "^3.1.5",
    "husky": "^1.2.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.0",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "react-testing-library": "^5.4.0",
    "styled-components": "^4.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  }
}
